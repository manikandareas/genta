                          "type": "object",
                          "properties": {
                            "status": {
                              "type": "string"
                            },
                            "response_time": {
                              "type": "string"
                            },
                            "error": {
                              "type": "string"
                            }
                          },
                          "required": ["status", "response_time"]
                        }
                      },
                      "required": ["database"]
                    }
                  },
                  "required": ["status", "timestamp", "environment", "checks"]
                }
              }
            }
          }
        }
      }
